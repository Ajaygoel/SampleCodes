@{
    PageData["title"] = "cargoclix | Bookings";
}
<div class="col-sm-6 col-sm-offset-3 col-md-4 col-md-offset-4">
    <div class="row">
        <h3>Slot Bookings</h3>
    </div>
    <div class="row">
        <h4>Please Select:</h4>
    </div>
    <div class="row">
        <ul ng-init="GetBookings($root.UserInfo.MobileNumber)">
            <li ng-repeat="i in Bookings">
                <span>{{i.Time }}    {{ i.Place}}</span>
                <a href="#/bookingDetails/{{i.Id}}"><span class="glyphicon glyphicon-chevron-right"></span></a>
            </li>
        </ul>
        without a booked slot advice your Truk <a href=""><span class="glyphicon glyphicon-chevron-right"></span></a>
    </div>
</div>