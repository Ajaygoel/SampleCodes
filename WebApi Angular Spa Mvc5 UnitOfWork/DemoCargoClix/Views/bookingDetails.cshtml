@{
    PageData["title"] = "Booking Details";
}
<div class="col-sm-6 col-sm-offset-3 col-md-4 col-md-offset-4">
    <h3> Selected Slot Booking</h3>
    <div class="row" ng-init="GetBookingDetails()">
        <div class="row">
            <h5>{{$root.BookingDetail.Time }}    {{$root.BookingDetail.Place}}</h5>
        </div>
        <div class="row">
            <span><a href="#/booking/status/onWay">I am on My Way<span class="glyphicon glyphicon-chevron-right"></span></a></span>
        </div>

        <div class="row">
            <span><a href="#/booking/status/reportDelay">Report Delay<span class="glyphicon glyphicon-chevron-right"></span></a></span>
        </div>
        <div class="row">
            <span><a href="#/booking/status/sendMessage">Enter Message<span class="glyphicon glyphicon-chevron-right"></span></a></span>
        </div>
    </div>
</div>